    "mqtt": "~4.3.7",
    "newman": "5.3.2",
    "newman-reporter-html": "1.0.5",
    "newman-reporter-htmlextra": "^1.22.11",
    "newman-reporter-teamcity": "^0.1.12",
    "node-fetch": "2.6.7",
    "nyc": "15.1.0",
    "prettier": "2.8.4",
    "rewire": "5.0.0",
    "rimraf": "3.0.2",
    "sinon": "10.0.0",
    "sinon-chai": "3.7.0",
    "source-map-support": "^0.5.21",
    "typescript": "^4.9.4",
    "why-is-node-still-running": "^1.0.0"
  },
  "overrides": {
    "eslint-plugin-radar": {
      "eslint": "8.22.0"
    },
