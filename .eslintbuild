{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:prettier/recommended"],
  "plugins": ["radar"],
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  },
  "rules": {
    "no-unused-vars": "off",
    "no-console": "off",
    "no-control-regex": "off",
    "no-case-declarations": "off",
    "radar/cognitive-complexity": ["error", 150],
    "radar/no-useless-catch": "off",
    "radar/no-identical-functions": "off",
    "radar/no-duplicated-branches": "off",
    "radar/no-redundant-jump": "off"
  }
}
